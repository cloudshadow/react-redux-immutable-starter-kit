{
  "name": "react-redux-immutable-starter-kit",
  "version": "1.0.0",
  "description": "starter kit for react v16",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "start:dev": "webpack-dev-server --color --config ./webpack.config.dev.js",
    "start:prod": "npm run clean-dist && webpack --config ./webpack.config.prod.js",
    "start:stable": "npm run clean-dist-stable && webpack --config ./webpack.config.stable.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "clean-dist-stable": "npm run remove-dist-stable && mkdir dist-stable",
    "remove-dist": "rimraf ./dist",
    "remove-dist-stable": "rimraf ./dist-stable",
    "generate": "babel-node tools/generateComponent/generateComponent.js"
  },
  "author": "CloudShadow.Me",
  "license": "MIT",
  "dependencies": {
    "axios": "0.15.3",
    "es6-promise": "4.1.0",
    "history": "^4.7.2",
    "immutable": "^4.0.0-rc.12",
    "object-assign": "4.1.0",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "5.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-slick": "^0.22.3",
    "redux": "3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "2.1.0",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "autoprefixer": "6.5.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "18.0.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "6.20.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-stage-1": "6.16.0",
    "browser-sync": "2.18.5",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.15",
    "css-loader": "^0.28.11",
    "enzyme": "2.6.0",
    "eslint": "3.12.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.8.0",
    "eslint-watch": "2.1.14",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.7",
    "identity-obj-proxy": "3.0.0",
    "json-loader": "0.5.4",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.0.0",
    "npm-run-all": "3.1.2",
    "open": "0.0.5",
    "postcss-loader": "^2.1.2",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.4.1",
    "redux-immutable-state-invariant": "1.2.4",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.11.1",
    "webpack-bundle-analyzer": "2.1.1",
