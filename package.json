{
  "name": "react-redux-immer-starter-kit",
  "version": "1.0.0",
  "description": "starter kit for react v17",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "start:dev": "webpack serve --color --config ./webpack.config.dev.js",
    "start:prod": "npm run clean-dist && webpack --config ./webpack.config.prod.js",
    "start:stable": "npm run clean-dist-stable && webpack --config ./webpack.config.stable.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "clean-dist-stable": "npm run remove-dist-stable && mkdir dist-stable",
    "remove-dist": "rimraf ./dist",
    "remove-dist-stable": "rimraf ./dist-stable",
    "generate": "babel-node tools/generateComponent/generateComponent.js"
  },
  "author": "CloudShadow.Me",
  "license": "MIT",
  "dependencies": {
    "axios": "0.21.0",
    "connected-react-router": "^6.8.0",
    "es6-promise": "4.2.8",
    "history": "^5.0.0",
    "immer": "8.0.0",
    "object-assign": "4.1.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "autoprefixer": "10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "^8.2.1",
    "browser-sync": "2.26.13",
    "chalk": "4.1.0",
    "css-loader": "^5.0.1",
    "eslint": "7.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-watch": "7.0.0",
    "file-loader": "^6.2.0",
    "happypack": "^5.0.1",
    "html-webpack-plugin": "^4.5.0",
    "json-loader": "0.5.7",
    "mini-css-extract-plugin": "^1.3.1",
    "node-sass": "^5.0.0",
    "npm-run-all": "4.1.5",
    "postcss-loader": "^4.0.4",
    "prompt": "1.0.0",
    "replace": "1.2.0",
    "rimraf": "3.0.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.0.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.4.0",
    "webpack-bundle-analyzer": "4.1.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "keywords": [
    "react",
    "reactjs",
    "immer",
    "react-router",
    "webpack",
    "redux",
    "boilerplate",
    "starter",
    "kit"
  ]
}
